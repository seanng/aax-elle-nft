generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Image {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  previewUrl      String
  message         String?
  nftId           String?
  contractAddress String?
  createdAt       DateTime @default(now())
  updatedAt       DateTime @updatedAt
}
